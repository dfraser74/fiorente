city\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay&&bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-md-12 mdpb col-lg-5\" mbr-class=\"{'order-lg-2': reverseContent}\">\n                <div class=\"card-wrap\">\n                <div class=\"card-box\">\n                 <h3 class=\"mbr-section-subtitle mbr-white mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubTitle\"><a href=\"index.html\" class=\"text-warning\">Home</a> &gt; <a href=\"book-a-consultation.html\" class=\"text-warning text-primary\">Consultation</a></h3>\n                \n                      <div class=\"title-wrap pb-4\" mbr-if=\"showTitle\">\n                        <h4 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".card-title, .title-wrap\">Bookings<br></h4>\n                            </div>\n                    \n                    \n                <p class=\"mbr-text pb-2 mbr-white mbr-light mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">\n                    Donec et nibh a risus aliquam</p>\n             </div>\n                </div>\n            </div>\n            \n             <div class=\"col-md-12 col-lg-7\">\n               <img src=\"@PROJECT_PATH@/assets/images/mbr-1266x844.jpg\" alt=\"Book a Consultation\" title>\n            </div>\n            \n            \n        </div>\n    </div>\n\n</section>",
